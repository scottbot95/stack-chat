{
  "extends": ["eslint-config-airbnb", "prettier", "prettier/react"],
  "plugins": ["babel"],
  "parser": "babel-eslint",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "no-console": 0,
    "no-plusplus": 0
  }
}
